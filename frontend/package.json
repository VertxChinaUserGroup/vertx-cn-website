{
  "name": "frontend",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "global": "sudo npm i -g npm yarn -r https://registry.npm.taobao.org && npm run init",
    "init": "rm -rf node_modules yarn.lock && yarn gfw",
    "gfw": "yarn add -D cross-env && cross-env PHANTOMJS_CDNURL=http://npm.taobao.org/mirrors/phantomjs yarn",
    "build": "npm run lint && npm run clean && npm run compile",
    "build:mock": "npm run lint:fix && npm run clean && npm run compile:mock",
    "build:test": "npm run lint && npm run clean && npm run compile:test",
    "clean": "cross-env DEBUG=vertx* node build/bin/clean",
    "compile": "cross-env NODE_ENV=production DEBUG=vertx* node build/bin/compile",
    "compile:mock": "cross-env NODE_ENV=production DEBUG=vertx* node build/bin/compile --mock",
    "compile:test": "cross-env NODE_ENV=test DEBUG=vertx* node build/bin/compile",
    "compile:test-mock": "cross-env NODE_ENV=test DEBUG=vertx* node build/bin/compile --mock",
    "debug": "npm run dev -- --debug",
    "dev": "cross-env DEBUG=vertx* PORT=4000 nodemon build/bin/start --mock",
    "dev:proxy": "cross-env DEBUG=vertx* PORT=3000 nodemon build/bin/start --proxy",
    "mock": "cross-env DEBUG=vertx* PORT=4000 nodemon build/bin/start --mock",
    "eslint": "eslint --max-warnings 10 --ext .js,.vue .",
    "lint": "npm run eslint && npm run stylelint && npm run puglint",
    "lint:fix": "npm run eslint -- --fix && npm run stylelint && npm run puglint",
    "lint:stage": "bash pre-commit.sh",
    "prod": "npm run compile:mock && npm run start",
    "start": "cross-env NODE_ENV=production DEBUG=vertx* node build/bin/start",
    "stylelint": "node build/bin/stylelint.js",
    "test": "npm run compile:test && npm run start:test",
    "test:mock": "npm run compile:test-mock && npm run start:test",
    "puglint": "pug-lint src"
  },
  "pre-commit": [
    "lint:stage"
  ],
  "dependencies": {
    "axios": "^0.16.1",
    "bootstrap-styl": "^5.0.8",
    "fonts.css": "^1.5.0",
